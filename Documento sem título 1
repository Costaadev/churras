<!--use (nome bd);  -->
 
 create table pessoas (
nome varchar (30),
idade tinyint,
sexo char (1),
peso float,
altura float,
nacionalidade varchar (20)
);

describe pessoas;

create database cadastro 
default character set utf8
default collate utf8_general_ci; 

